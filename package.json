{
  "name": "simply-color-extraction",
  "version": "1.0.1",
  "description": "A library to extract dominant colors from images.",
  "type": "module",
  "scripts": {},
  "main": "src/core.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "import": "./src/core.js",
      "require": "./src/core.js",
      "types": "./types/index.d.ts"
    },
    "./browserAdapter": {
      "import": "./src/Adapter/browserAdapter.js",
      "require": "./src/Adapter/browserAdapter.js",
      "types": "./types/index.d.ts"
    },
    "./nodeAdapter": {
      "import": "./src/Adapter/nodeAdapter.js",
      "require": "./src/Adapter/nodeAdapter.js",
      "types": "./types/index.d.ts"
    }
  },
  "repository": "https://github.com/awesomelon/color-extraction",
  "keywords": [
    "color",
    "extractor",
    "image",
    "kmeans",
    "dominant colors",
    "typescript",
    "nodejs",
    "browser"
  ],
  "author": "j-ho",
  "license": "MIT",
  "dependencies": {
    "chroma-js": "^2.4.2",
    "ml-kmeans": "^6.0.0",
    "pureimage": "^0.4.13",
    "sharp": "^0.33.4",
    "tempfile": "^5.0.0"
  },
  "devDependencies": {
    "typescript": "^5.5.4"
  }
}
